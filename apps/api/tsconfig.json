{
   "compilerOptions": {
      "strict": true,
      "jsx": "react-jsx",
      "jsxImportSource": "hono/jsx",
      "skipLibCheck": true,
      "skipDefaultLibCheck": true,
      "noErrorTruncation": false,
      "incremental": true,
      "tsBuildInfoFile": ".tsbuildinfo"
   },
   "exclude": ["node_modules", ".tsbuildinfo"]
}
